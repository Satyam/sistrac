{
  "name": "sistract-desktop",
  "version": "0.0.0",
  "description": "Sistrac - Desktop Client Version",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "formatcoords": "^1.1.3",
    "leaflet": "^1.2.0",
    "parse-dms": "0.0.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-leaflet": "^1.7.8",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=$NODE_PATH:./src/_ react-scripts start",
    "build": "cross-env NODE_PATH=$NODE_PATH:./src/_ run-s build:clean build:compile",
    "build:clean": "rimraf build",
    "build:compile": "react-scripts build",
    "test": "cross-env NODE_PATH=$NODE_PATH:./src/_ react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js . || exit 0",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-mock": "^5.13.1",
    "jest-enzyme": "^4.0.2",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "rimraf": "^2.6.2",
    "source-map-explorer": "^1.5.0"
  },
  "jest": {
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
