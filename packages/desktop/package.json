{
  "name": "sistract-desktop",
  "version": "0.0.0",
  "description": "Sistrac - Desktop Client Version",
  "dependencies": {
    "@devasatyam/controls": "^1.0.0",
    "apollo-client-preset": "^1.0.8",
    "bootstrap": "^4.0.0",
    "bootswatch": "^4.0.0",
    "final-form": "^4.2.1",
    "formatcoords": "^1.1.3",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.8.0",
    "leaflet": "^1.3.1",
    "parse-dms": "0.0.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-final-form": "^3.1.0",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-leaflet": "^1.8.1",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "unstated": "^1.0.3"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=$NODE_PATH:./src/_ react-scripts start",
    "build": "cross-env NODE_PATH=$NODE_PATH:./src/_ run-s build:clean build:compile",
    "build:clean": "rimraf build",
    "build:compile": "react-scripts build",
    "test": "cross-env NODE_PATH=$NODE_PATH:./src/_ react-scripts test --env=jsdom",
    "test:debug": "cross-env NODE_PATH=$NODE_PATH:./src/_ react-scripts --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js . || exit 0",
    "analyze": "run-p analyze:main analyze:login analyze:estaciones analyze:estacion analyze:editEstacion",
    "analyze:main": "source-map-explorer build/static/js/main.*",
    "analyze:login": "source-map-explorer build/static/js/login.*",
    "analyze:estaciones": "source-map-explorer build/static/js/estaciones.*",
    "analyze:estacion": "source-map-explorer build/static/js/estacion.*",
    "analyze:editEstacion": "source-map-explorer build/static/js/editEstacion.*"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-mock": "^5.13.1",
    "jest-enzyme": "^4.2.0",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "source-map-explorer": "^1.5.0"
  },
  "jest": {
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
